<%= render partial: 'layouts/errors', locals: {object: @venue} %>
<%= form_for(@venue) do |f|%>
<%= f.label :venue, "Select a venue:" %>
<%= f.collection_select :venue_id, Venue.all, :id, :name, prompt: true %><br><br>
<%=f.label "City:"%>
<%=f.text_field :city%><br><br>
<%=f.label "State:"%>
<%=f.text_field :state%><br><br>
<%=f.label "Capacity:"%>
<%=f.number_field :capacity%>

<%= f.submit %>

<% end %>
